<div class="modal fade" id="editar-observacion-resumen" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content add-to-modal">
      <div class="modal-header modal-header-editar-observacion-resumen">
        <h5 class="modal-title">Editando observaciones del mes de <span class="spanMonthTitleModal"></span></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        <div style="display: block !important;">
          <h5 class="infoCatEditing"></h5>
        </div>
      </div>
      <div class="separador-modal">
      </div>
      <% if (observacionesResumen !== null) { %>
      <form action="/editar-observacion-resumen/<%=observacionesResumen._id%>" method="POST" id="form-editar-obs-resumen">
        <% } %>
        <div class="container-form-add-cat">
          <div class="form-floating mb-3 editObsEgreso">
            <% if (observacionesResumen !== null) { %>
            <textarea name="observaciones" class="form-control observaciones" id="observaciones" placeholder="Observaciones"><%= observacionesResumen.observaciones %></textarea>
            <label for="observaciones" class="form-label">Observaciones</label>
            <% } %>
          </div>
        </div>
        <div class="separador-modal">
        </div>
        <div class="modal-footer no-seleccionable">
          <button type="button" class="btn-cancelar-cambios-cat" data-bs-dismiss="modal">
            <span class="span-btn-cancelar-cambios">Cancelar</span>
          </button>
          <button type="submit" class="btn-editar-observaciones-resumen btn-cancelar-cambios-cat">
            <span>Guardar cambios</span>
          </button>
        </div>
    </div>
  </div>
</div>